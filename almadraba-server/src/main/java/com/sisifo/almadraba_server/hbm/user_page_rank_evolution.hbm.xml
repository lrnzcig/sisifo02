<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sisifo.almadraba_server.hbm">

    <class name="UserPageRankEvolution" table="user_page_rank_evolution">
        <composite-id name="id" class="UserPageRankEvolutionId">
	        <key-property name="userId" type="big_integer" column="user_id"/>
	        <key-property name="rankExecId" type="string" column="rank_exec_id"/>
    	    <key-property name="rankStepId" type="string" column="rank_step_id"/>
        </composite-id>
        <property name="rank" type="float" column="rank"/>
    </class>

</hibernate-mapping>